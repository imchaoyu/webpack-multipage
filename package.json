{
  "name": "webpack-multi-page",
  "version": "1.0.0",
  "description": "a multi-page webpack demo",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --color --progress --config build/webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --inline --color --progress --config build/webpack.prod.config.js",
    "server": "live-server ./ --port=8888",
    "start": "npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chaoyuwang/webpack-multipage.git"
  },
  "keywords": [
    "webpack"
  ],
  "author": "chaoyu",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^8.6.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "html-webpack-plugin": "^3.2.0",
    "html-withimg-loader": "^0.1.16",
    "imagemin-webpack-plugin": "^2.1.5",
    "live-server": "^1.2.0",
    "node-sass": "^4.9.0",
